ntext": [{"kleur": "zw/w"}], "laatst_aangepastdatum": "2014-05-08T15:05:56", "type": "Item", "id": "14447", "niveaus": [{"geografische_namen": ["Den Haag", "Nederland"], "trefwoorden": [{"trefwoord": "gaarkeukens"}], "persoonsnamen": ["Bilt, C.L. van der"], "taalID": "1", "intentieID": "1", "collectie_avails": ["Polygoon Hollands Nieuws"], "annotatie": "Avail: 1'05\"\nTussentitel(s).\nDit journaal is Tobis Hollands Nieuws vervaardigd door Profilti.\nPapieren: Titellijst Centrale Commissie Filmkeuring. Zie bijlage bij beschrijving.", "beschrijving": "Rij mensen die buiten in de rij staan met emmers, schalen, tassen en pannen, afgewisseld met beelden van koks en keukenpersoneel die bezig zijn met het klaarmaken van stamppot in grote ketels en gamellen. De waarnemend burgemeester van Den Haag, professor ir. C.L. van der Bilt, eet een paar happen van een bord om de maaltijd te keuren. Tegen inlevering van bonnen krijgen de wachtenden vervolgens hun eten mee in de zelf meegebrachte pannen, emmers etc.; \n00:30:40 Van der Bilt."}], "tijdsduur": 65000}], "facets": [], "reeks": {"aanmaakdatum": "2013-11-24T04:59:35", "aantalafleveringen": 0, "parent_id": "12070", "verwijderd": false, "id": "12070", "laatst_aangepastdatum": "2013-11-24T04:59:35"}, "keyframesets": [{"epguid": "fbe499d7-9047-4a47-90db-27e9fc65065f", "dmguid": "444C4755494400735315BA1D39270030F86A82030103000BE1A8C0F800000000", "numberOfKeyframes": 70, "timecodes": [40, 2880, 6520, 6600, 8160, 8760, 9360, 11520, 11600, 14360, 14400, 16640, 16720, 16920, 17680, 17840, 18000, 18200, 18520, 18600, 20840, 20920, 22160, 22200, 23200, 23920, 25480, 25560, 26320, 26360, 26640, 27640, 27720, 28840, 29400, 30480, 30600, 31520, 32800, 33520, 33560, 34560, 37800, 38520, 39600, 41040, 42640, 42680, 44800, 44880, 47320, 47400, 48360, 48920, 49480, 50000, 50440, 51920, 52000, 53200, 55200, 57320, 59080, 59320, 59440, 60600, 61280, 62440, 63560, 65640]}], "realisatie": {"titel": {"tekst": "Polygoon Hollands Nieuws", "bron": "A