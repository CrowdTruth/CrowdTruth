ies": ["commentaar"]}], "begintijd": 2984000, "niveaus": [{"geografische_namen": ["Rotterdam", "Nederland", "Coolsingel"], "trefwoorden": [{"trefwoord": "openingen"}, {"trefwoord": "wegen"}], "persoonsnamen": ["Manders, Tom"], "taalID": "1", "themas": ["Tranche POL goedgekeurd"], "intentieID": "1", "collectie_avails": ["Polygoon Hollands Nieuws"], "beschrijving": "Tom Manders komt als Dorus in een oldtimer aan bij de\nfeestelijk versierde Coolsingel en legt de laatste steen in\nhet plaveisel. Vervolgens rijdt hij in zijn auto een aantal\nballonnen kapot, met naast hem een vrouw die op 14 mei 1940,\nde dag van het bombardement op Rotterdam, is geboren, waarna\nhij over de Coolsingel rijdt. Met verder beelden van mensen\ndie het metrostation Stadhuis bekijken en instappen in een\nmetro. Met archiefmateriaal van de aanleg van de metro aan de\nCoolsingel.", "vervallen_trefwoorden": ["metro"]}], "context": [{"kleur": "zw/w; kleur"}], "id": "395186", "laatst_aangepastdatum": "2013-11-24T06:44:38", "tijdsduur": 131000, "type": "Item", "opnamen": [{"begindatum": "13-10-1967"}], "verwijderd": false, "titel": {"tekst": "VERKEERSCHAOS IN ROTTERDAM TEN EINDE\nOFFICIELE HEROPENING COOLSINGEL"}}], "facets": [], "reeks": {"aanmaakdatum": "2013-11-24T04:59:35", "aantalafleveringen": 0, "parent_id": "12070", "verwijderd": false, "id": "12070", "laatst_aangepastdatum": "2013-11-24T04:59:35"}, "realisatie": {"titel": {"tekst": "Polygoon Hollands Nieuws", "bron": "Avail TITEL", "type": "Titel"}, "distributiekanaal": "bioscoop", "aanmaakdatum": "2013-11-24T04:59:35", "overige_titels": [{"tekst": "polhn#Polygoon Hollands Nieuws", "bron": "Prodic", "type": "ProdicTitel"}], "parent_id": "12068", "niveaus": [{"genres": ["journaal"], "taalID": "1", "intentieID": "1", "samenvatting": "Bioscoopjournaals waarin Nederlandse onderwerpen van een bepaalde week worden gepresenteerd.", "deelcatalogi": ["Bewegend beeld/Polygoon/Hollands nieuws"], "taakID": "28573"}, {"beschrijving": "Het Polygoon-journaal is decennialang h\u00e9t bioscoopjournaal in Nederland. In januari 1922 beginnen de eerste wekelijkse filmjournaals onder de titel 'Hollandsch nieuws'. Tot 1986 blijft het Polygoonjournaal bestaan. Daarna stopt het, omdat het bioscoopjournaal definitief is 'ingehaald' door de dagelijkse televisiejournaals. Philip Bloemendal (1918-1999) is veertig jaar lang d\u00e9 stem van Polygoon.", "genres": ["Nieuws"], "taalID": "1", "taakID": "29772", "intentieID": "2"}], "laatst_aangepastdatum": "2013-11-24T04:59:35", "id": "12070", "verwijderd": false}, "werk": {"titel": {"tekst": "Polygoon bioscoopjournaal", "type": "uniforme titel"}, "aanmaakdatum": "2013-11-24T05:02:23", "overige_titels": [{"tekst": "polhn#Polygoon bioscoopjournaal", "bron": "Prodic", "type": "ProdicTitel"}], "niveaus": [{"taalID": "1", "taakID": "28553", "intentieID": "1"}], "laatst_aangepastdatum": "2013-11-24T05:02:23", "id": "12068", "verwijderd": false}}