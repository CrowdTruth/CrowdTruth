"niveaus": [{"geografische_namen": ["Nederland", "Nederlands-Indi\u00eb", "Amsterdam"], "trefwoorden": [{"trefwoord": "dokken"}, {"trefwoord": "scheepsbouw"}, {"trefwoord": "scheepvaart"}, {"trefwoord": "schepen"}], "namen": ["Johan De Witte", "Nederlandse Dok Maatschappij"], "taalID": "1", "intentieID": "1", "collectie_avails": ["Polygoon Hollands Nieuws"], "annotatie": "Avail: 63\"\n\n\nPapieren: bron: Algemeen Handelsblad 10:04:1924", "beschrijving": "Beeldverslag van de open dag tgv het gereed komen van drie\ngrote dokken van de Nederlandse Dokmaatschappij gelegen aan\nde Noord over van het Noord-kanaal. Bij deze gelegenheid werd\neen rondleiding gehouden waaraan werd deelgenomen door tal\nvan Amsterdamse en Rotterdamse directeuren uit het bankwezen,\nde scheepvaart en de gemeenten waaronder bijvoorbeeld J. ter\nHaar, wethouder publieke werken Amsterdam, mr. G. Visser,\npresident-directeur Nederlandse Bank en kapitein ter zee\nG.H.A. Greve. Na afloop werd er thee geserveerd op de \"Johan\nde Witte\".\nShots : Overzichtsshot nieuwe dokken 23\"; bezoekers bekijken\nterrein en int. nieuwe dokken; theedrinken op de \"Johan de\nWitte\"; poseren drie Indische bedienden 5\"."}], "tijdsduur": 63000}], "facets": [], "reeks": {"aanmaakdatum": "2013-11-24T04:59:35", "aantalafleveringen": 0, "parent_id": "12070", "verwijderd": false, "id": "12070", "laatst_aangepastdatum": "2013-11-24T04:59:35"}, "keyframesets": [{"epguid": "821f40bf-f511-4ffb-86d6-e889599abc78", "dmguid": "444C4755494400735315BA1D39270030F86A82030103000BE1A8C05C07000000", "numberOfKeyframes": 48, "timecodes": [0, 600, 1960, 2160, 2280, 4680, 4720, 6120, 6880, 7200, 7240, 7520, 9240, 9280, 14040, 14760, 15040, 17200, 17400, 17560, 18640, 19000, 20080, 20800, 22640, 24720, 24760, 24960, 29000, 36880, 39240, 39560, 39640, 41240, 43360, 43400, 43520, 45440, 45680, 45760, 46040, 48440, 48480, 48680, 52360, 52720, 57560, 61920]}], "realisatie": {"titel": {"tekst": "Polygoon Hollands Nieuws", "bron": "Avail TITEL", "type": "Titel"}, "distributiekanaal": "bioscoop", "aanmaakdatum": "2013-11-24T04:59:35", "overige_titels": [{"tekst": "polhn#Polygoon Hollands Nieuws", "bron": "Prodic", "type": "ProdicTitel"}], "parent_id": "12068", "niveaus": [{"genres": ["journaal"], "taalID": "1", "intentieID": "1", "samenvatting": "Bioscoopjournaals waarin Nederlandse onderwerpen van een bepaalde week worden gepresenteerd.", "deelcatalogi": ["Bewegend beeld/Polygoon/Hollands nieuws"], "taakID": "28573"}, {"beschrijving": "Het Polygoon-journaal is decennialang h\u00e9t bioscoopjournaal in Nederland. In januari 1922 beginnen de eerste wekelijkse filmjournaals onder de titel 'Hollandsch nieuws'. Tot 1986 blijft het Polygoonjournaal bestaan. Daarna stopt het, omdat het bioscoopjournaal definitief is 'ingehaald' door de dagelijkse televisiejournaals. Philip Bloemendal (1918-1999) is veertig jaar lang d\u00e9 stem van Polygoon.", "genres": ["Nieuws"], "taalID": "1", "taakID": "29772", "intentieID": "2"}], "laatst_aangepastdatum": "2013-11-24T04:59:35", "id": "12070", "verwijderd": false}, "werk": {"titel": {"tekst": "Polygoon bioscoopjournaal", "type": "uniforme titel"}, "aanmaakdatum": "2013-11-24T05:02:23", "overige_titels": [{"tekst": "polhn#Polygoon bioscoopjournaal", "bron": "Prodic", "type": "ProdicTitel"}], "niveaus": [{"taalID": "1", "taakID": "28553", "intentieID": "1"}], "laatst_aangepastdatum": "2013-11-24T05:02:23", "id": "12068", "verwijderd": false}}