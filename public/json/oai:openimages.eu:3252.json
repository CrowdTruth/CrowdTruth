orden": [{"trefwoord": "ambulances"}, {"trefwoord": "chirurgen"}, {"trefwoord": "operaties"}, {"trefwoord": "stadsverkeer"}, {"trefwoord": "verkeersonderwijs"}, {"trefwoord": "verkeersongevallen"}, {"trefwoord": "verkeersovertredingen"}], "persoonsnamen": ["Vries, Rob de"], "taalID": "1", "themas": ["Tranche POL goedgekeurd"], "intentieID": "1", "collectie_avails": ["Polygoon Hollands Nieuws"], "annotatie": "RVD-nummer: 099-215\n37:19 dubbel beeld \u00e9n te donker; gevolgd door twee seconden\nzwart.\nPapieren: Commentaartekst uit register Centrale Commissie\nFilmkeuring.\nZie bijlage bij beschrijving.", "beschrijving": "Verkeersles voor de kijkers. \nSHOTS: \neen verkeersdeelnemer, Jansen genaamd (gespeeld door de acteur\nRob de Vries), belandt door eigen stommiteit (als fietser geen\nvoorrrang verleend aan snelverkeer) in het ziekenhuis. Nav\nzijn ongeval wordt een aantal voorrangssituaties in beeld\ngebracht m\u00e9t daarbij voorbeelden van hoe w\u00e9l en hoe n\u00edet te\nhandelen. Waarschuwende beelden zijn meegemonteerd: Jansen\nligt met gespalkt been in bed, aanrijding tussen fietser en\nautomobilist, ambulance voert het slachtoffer af, kerkhof met\ngraven, verpleegsters rijden verkeersslachtoffer op een\nbrancard naar de operatiekamer, chirurgische instrumenten\nliggen klaar, operatieteam is aan het werk."}], "titel": {"tekst": "WIE HEEFT VOORRANG?"}}], "facets": [], "reeks": {"aanmaakdatum": "2013-11-24T04:59:35", "aantalafleveringen": 0, "parent_id": "12070", "verwijderd": false, "id": "12070", "laatst_aangepastdatum": "2013-11-24T04:59:35"}, "realisatie": {"titel": {"tekst": "Polygoon Hollands Nieuws", "bron": "Avail TITEL", "type": "Titel"}, "distributiekanaal": "bioscoop", "aanmaakdatum": "2013-11-24T04:59:35", "overige_titels": [{"tekst": "polhn#Polygoon Hollands Nieuws", "bron": "Prodic", "type": "ProdicTitel"}], "parent_id": "12068", "niveaus": [{"genres": ["journaal"], "taalID": "1", "intentieID": "1", "samenvatting": "Bioscoopjournaals waarin Nederlandse onderwerpen van een bepaalde week worden gepresenteerd.", "deelcatalogi": ["Bewegend beeld/Polygoon/Hollands nieuws"], "taakID": "28573"}, {"beschrijving": "Het Polygoon-journaal is decennialang h\u00e9t bioscoopjournaal in Nederland. In januari 1922 beginnen de eerste wekelijkse filmjournaals onder de titel 'Hollandsch nieuws'. Tot 1986 blijft het Polygoonjournaal bestaan. Daarna stopt het, omdat het bioscoopjournaal definitief is 'ingehaald' door de dagelijkse televisiejournaals. Philip Bloemendal (1918-1999) is veertig jaar lang d\u00e9 stem van Polygoon.", "genres": ["Nieuws"], "taalID": "1", "taakID": "29772", "intentieID": "2"}], "laatst_aangepastdatum": "2013-11-24T04:59:35", "id": "12070", "verwijderd": false}, "werk": {"titel": {"tekst": "Polygoon bioscoopjournaal", "type": "uniforme titel"}, "aanmaakdatum": "2013-11-24T05:02:23", "overige_titels": [{"tekst": "polhn#Polygoon bioscoopjournaal", "bron": "Prodic", "type": "ProdicTitel"}], "niveaus": [{"taalID": "1", "taakID": "28553", "intentieID": "1"}], "laatst_aangepastdatum": "2013-11-24T05:02:23", "id": "12068", "verwijderd": false}}