, "beginopdrager": 312000}, {"datum_aangemaakt": "2008-04-11T23:30:09", "dragersoort": "Programma", "volgnummer": 1, "eindeopdrager": 437000, "annotatie": "False", "positieid": "1451645", "dragernummer": "61-30", "dragertype": "wknr.", "technische_gegevens": "Materiaal: zw/w\nFilm     : 35mm\nGeluid   : CO", "dragerid": "1158406", "publid": "-1", "beginopdrager": 312000}], "parent_id": "412138", "broncatalogus": "Polygoon Hollands Nieuws", "aanmaakdatum": "2013-11-24T05:22:08", "maker": [{"naam": "Bloemendal, Philip", "functies": ["commentaar"]}], "begintijd": 312000, "niveaus": [{"geografische_namen": ["Nederland", "Amersfoort"], "trefwoorden": [{"trefwoord": "archeologie"}, {"trefwoord": "opgravingen"}], "namen": ["Rijksdienst voor het Oudheidkundig Bodemonderzoek"], "taalID": "1", "themas": ["Tranche POL goedgekeurd"], "intentieID": "1", "collectie_avails": ["Polygoon Hollands Nieuws"], "beschrijving": "Een in november 1960 gevonden Romeins graf uit 200 na\nChristus wordt rondom geheel uitgegraven en van een mantel\nvoorzien, waarna er een plaat onder wordt getrokken.\nVervolgens wordt het geheel met hijskranen op een vrachtwagen\ngetild en naar een garage van de Rijksdienst voor\nOudheidkundig Bodemonderzoek in Amersfoort gebracht. Daar\ngraven archeologen het graf voorzichtig uit, waarbij div.\nvoorwerpen aan het licht komen.", "vervallen_trefwoorden": ["bijzondere transporten"]}], "context": [{"kleur": "zw/w"}], "id": "378955", "laatst_aangepastdatum": "2014-05-08T14:58:11", "tijdsduur": 125000, "type": "Item", "opnamen": [{"begindatum": "13-07-1961"}], "verwijderd": false, "titel": {"tekst": "ROMEINS GRAF BLOOTGELEGD"}}], "facets": [], "reeks": {"aanmaakdatum": "2013-11-24T04:59:35", "aantalafleveringen": 0, "parent_id": "12070", "verwijderd": false, "id": "12070", "laatst_aangepastdatum": "2013-11-24T04:59:35"}, "realisatie": {"titel": {"tekst": "Polygoon Hollands Nieuws", "bron": "Avail TITEL", "type": "Titel"}, "distributiekanaal": "bioscoop", "aanmaakdatum": "2013-11-24T04:59:35", "overige_titels": [{"tekst": "polhn#Polygoon Hollands Nieuws", "bron": "Prodic", "type": "ProdicTitel"}], "parent_id": "12068", "niveaus": [{"genres": ["journaal"], "taalID": "1", "intentieID": "1", "samenvatting": "Bioscoopjournaals waarin Nederlandse onderwerpen van een bepaalde week worden gepresenteerd.", "deelcatalogi": ["Bewegend beeld/Polygoon/Hollands nieuws"], "taakID": "28573"}, {"beschrijving": "Het Polygoon-journaal is decennialang h\u00e9t bioscoopjournaal in Nederland. In januari 1922 beginnen de eerste wekelijkse filmjournaals onder de titel 'Hollandsch nieuws'. Tot 1986 blijft het Polygoonjournaal bestaan. Daarna stopt het, omdat het bioscoopjournaal definitief is 'ingehaald' door de dagelijkse televisiejournaals. Philip Bloemendal (1918-1999) is veertig jaar lang d\u00e9 stem van Polygoon.", "genres": ["Nieuws"], "taalID": "1", "taakID": "29772", "intentieID": "2"}], "laatst_aangepastdatum": "2013-11-24T04:59:35", "id": "12070", "verwijderd": false}, "werk": {"titel": {"tekst": "Polygoon bioscoopjournaal", "type": "uniforme titel"}, "aanmaakdatum": "2013-11-24T05:02:23", "overige_titels": [{"tekst": "polhn#Polygoon bioscoopjournaal", "bron": "Prodic", "type": "ProdicTitel"}], "niveaus": [{"taalID": "1", "taakID": "28553", "intentieID": "1"}], "laatst_aangepastdatum": "2013-11-24T05:02:23", "id": "12068", "verwijderd": false}}