: [{"geografische_namen": ["Nederland", "Ierland", "Shannon"], "trefwoorden": [{"trefwoord": "persconferenties"}, {"trefwoord": "rubberboten"}, {"trefwoord": "vliegtuigongevallen"}], "persoonsnamen": ["Viruly, Adriaan", "Aler, I.A."], "namen": ["Lockheed Super-Constellation L-749"], "taalID": "1", "themas": ["Tranche POL goedgekeurd"], "intentieID": "1", "collectie_avails": ["Polygoon Hollands Nieuws"], "beschrijving": "De Super-Constellation Triton ligt in de rivier Shannon bij\nde gelijknamige plaats in Ierland, na kort na het opstijgen\nte zijn neergestort. Op de oever zijn de voetsporen van de\ndrenkelingen nog zichtbaar, alsmede enkele rubberboten in het\nriet. 22 passagiers en zeven bemanningsleden hebben zich\nweten te redden, 24 passagiers en drie bemanningsleden zijn\nbij de ramp omgekomen. Leden van de Nederlandse en Ierse\nluchtvaartinspectie varen met boten rondom het wrak. Enkele\ndagen na de ramp arriveren de radiotelegrafist Henny\nOudshoorn, de 2e vlieger Evert Webbink, die de verkeerstoren\nin Shannon waarschuwde, de 2e boordwerktuigkundige Cornelis\nKievits en de 3e vlieger Johan Tieman in geleende\nburgerkleren terug op Schiphol. Twee dagen later arriveren de\ngezagvoerder Viruly, 1e vlieger Parritt en 1e\nboordwerktuigkundige Rademaker met een Super Constellation\nterug op Schiphol. De heer I.A. Aler, president-directeur van\nde KLM, houdt een persconferentie."}], "context": [{"kleur": "zw/w"}], "id": "381999", "laatst_aangepastdatum": "2014-05-08T14:59:20", "tijdsduur": 258000, "type": "Item", "opnamen": [{"begindatum": "07-09-1954"}], "verwijderd": false, "titel": {"tekst": "DE SUPER-CONSTELLATION \"TRITON\" BIJ SHANNON VERONGELUKT"}}], "facets": [], "reeks": {"aanmaakdatum": "2013-11-24T04:59:35", "aantalafleveringen": 0, "parent_id": "12070", "verwijderd": false, "id": "12070", "laatst_aangepastdatum": "2013-11-24T04:59:35"}, "realisatie": {"titel": {"tekst": "Polygoon Hollands Nieuws", "bron": "Avail TITEL", "type": "Titel"}, "distributiekanaal": "bioscoop", "aanmaakdatum": "2013-11-24T04:59:35", "overige_titels": [{"tekst": "polhn#Polygoon Hollands Nieuws", "bron": "Prodic", "type": "ProdicTitel"}], "parent_id": "12068", "niveaus": [{"genres": ["journaal"], "taalID": "1", "intentieID": "1", "samenvatting": "Bioscoopjournaals waarin Nederlandse onderwerpen van een bepaalde week worden gepresenteerd.", "deelcatalogi": ["Bewegend beeld/Polygoon/Hollands nieuws"], "taakID": "28573"}, {"beschrijving": "Het Polygoon-journaal is decennialang h\u00e9t bioscoopjournaal in Nederland. In januari 1922 beginnen de eerste wekelijkse filmjournaals onder de titel 'Hollandsch nieuws'. Tot 1986 blijft het Polygoonjournaal bestaan. Daarna stopt het, omdat het bioscoopjournaal definitief is 'ingehaald' door de dagelijkse televisiejournaals. Philip Bloemendal (1918-1999) is veertig jaar lang d\u00e9 stem van Polygoon.", "genres": ["Nieuws"], "taalID": "1", "taakID": "29772", "intentieID": "2"}], "laatst_aangepastdatum": "2013-11-24T04:59:35", "id": "12070", "verwijderd": false}, "werk": {"titel": {"tekst": "Polygoon bioscoopjournaal", "type": "uniforme titel"}, "aanmaakdatum": "2013-11-24T05:02:23", "overige_titels": [{"tekst": "polhn#Polygoon bioscoopjournaal", "bron": "Prodic", "type": "ProdicTitel"}], "niveaus": [{"taalID": "1", "taakID": "28553", "intentieID": "1"}], "laatst_aangepastdatum": "2013-11-24T05:02:23", "id": "12068", "verwijderd": false}}