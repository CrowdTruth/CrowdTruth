ontext": [{"indicatie_stom": true, "kleur": "zw/w"}], "laatst_aangepastdatum": "2014-05-08T15:04:18", "opnamen": [{"annotatie": "(exacte datum onbekend)", "datumprecisie": "exacte datum onbekend", "begindatum": "01-01-1943"}], "niveaus": [{"genres": ["onderwijsfilm"], "geografische_namen": ["Nederland", "Wieringermeer"], "trefwoorden": [{"trefwoord": "infrastructuur"}, {"trefwoord": "polders"}], "taalID": "1", "themas": ["Tranche SFWBB goedgekeurd"], "intentieID": "1", "collectie_avails": ["SFW Bewegend beeld"], "samenvatting": "Film over de Zuiderzeepolders. Aan de hand van een autorit\nvan de rentmeester van de Wieringermeer wordt getoond hoe\ngecultiveerd het landschap, met bruggen, wegen, woningen,\ndijken en kanalen, na tien jaar na het droogvallen van de\nWieringenmeer is.", "taakID": "948013"}, {"taalID": "1", "themas": ["Het verdwenen leven / Tweede Wereldoorlog en jaren 40 / Tweede en Wereldoorlog en jaren 40"], "intentieID": "2", "samenvatting": "Het werk van de rentmeester in de Wieringermeer, jaren '40.", "taakID": "948014", "beschrijving": "Na tien jaar harde arbeid ligt de Wieringermeerpolder er strak bij. Enorme boerderijen zijn de blikvangers in een gigantisch verkaveld en gecultiveerd landschap. Zo ver je kunt kijken ligt het land er met zijn gewassen prachtig bij.\n\nTeelt\nWe zien de rentmeester van de Wieringermeer aan het werk. Aan de hand van de verschillende soorten ankergrond bepaalt hij welke gewassen waar kunnen worden geplant. Zo vragen de teelt van vlas, bonen, tarwe en haver allemaal om specifieke, vruchtbare grond."}], "tijdsduur": 707000}, "selecties": [], "facets": [], "reeks": {"aanmaakdatum": "2013-11-23T03:05:56", "aantalafleveringen": 0, "parent_id": "188204", "verwijderd": false, "id": "194496", "laatst_aangepastdatum": "2013-11-23T03:05:56"}, "keyframesets": [{"epguid": "42e44880-26ad-4ee0-b0c4-7e075eabadd4", "dmguid": "2009040112451570800790450740014C2607C530000000540B00000D0F009457", "numberOfKeyframes": 647, "timecodes": [0, 